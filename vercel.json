{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"   # Vercel generar√° la carpeta `dist` al compilar
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ]
  }
  